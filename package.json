{
  "name": "touch-alt",
  "version": "0.3.0",
  "description": "Create from a template instead of a new file",
  "license": "MIT",
  "repository": "akameco/touch-alt",
  "author": {
    "name": "akameco",
    "email": "akameco.t@gmail.com",
    "url": "akameco.github.io"
  },
  "bin": "cli.js",
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "precommit": "lint-staged",
    "prettier": "prettier --write --single-quote --no-semi '{index,cli,test}.js'",
    "test": "xo && ava"
  },
  "files": [
    "cli.js",
    "index.js"
  ],
  "keywords": [
    "cli"
  ],
  "lint-staged": {
    "*.js": [
      "prettier",
      "git add"
    ]
  },
  "dependencies": {
    "cp-file": "^4.2.0",
    "make-dir": "^1.0.0",
    "meow": "^3.7.0",
    "path-exists": "^3.0.0",
    "update-notifier": "^2.1.0"
  },
  "devDependencies": {
    "ava": "^0.19.1",
    "del": "^2.2.2",
    "husky": "^0.13.3",
    "lint-staged": "^3.4.1",
    "prettier": "^1.3.1",
    "uuid": "^3.0.1",
    "xo": "^0.18.2"
  },
  "xo": {
    "space": true,
    "semicolon": false,
    "rules": {
      "object-curly-spacing": 0
    }
  }
}
